{
  "name": "project-2",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {},
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "https://project2backendtest.herokuapp.com/*",
    "http://localhost:8080/*",
    "contextMenus"
  ]
}
